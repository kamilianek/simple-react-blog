{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "class-methods-use-this": [0],
    "no-console": [0],
    "no-underscore-dangle": ["error", { "allow": ["_id", "_value"] }],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0],
    "jsx-a11y/href-no-hash": "off"
  },
  "globals": {
    "__DEV__": true
  }
}